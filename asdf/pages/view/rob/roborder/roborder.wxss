/* pages/orderDetail/orderDetail.wxss */
.container{padding:0 30rpx; background: #fff;}

.product{overflow: hidden;padding:30rpx 0;}
.product .logo{width:160rpx; height: 160rpx; float:left}
.product .productContent{ margin-left:180rpx; position: relative;}
.productContent text{display: block; font-size:30rpx;}
.productContent .title{color:#000; font-size:36rpx; margin-top:-15rpx; padding:10rpx 0}
.productContent .phoneIcon{position: relative; right:-70px; width:45rpx;height: 45rpx;vertical-align:-5rpx;}
.order {padding:20rpx 0 0}
.subcell {padding-left:0}
.order .weui-cell__ft {padding:0; font-size:26rpx; color:#727171}
.order .weui-cell__ft:after {border-width:0; }
.radio-group label {width:100%;}

.serverNews{padding:30rpx 0 100rpx;}
.serverNews .item{ overflow: hidden; font-size: 28rpx; padding:15rpx 0px;}
.serverNews .item .title{color:#727171; float: left; width:150rpx;}
.serverNews .item .content{margin-left:150rpx; text-align: left;}


.noteTitle{font-size:32rpx; color:#000; border-top:1px solid #c7c7cc; margin-top:20rpx; padding:30rpx 0 20rpx;}
.noteTitle .tips{color:#f00; font-size:20rpx;}

.jdbox {background:#fff; z-index:11; position:fixed; width:100%; padding-bottom:60rpx; bottom:0rpx; box-sizing:border-box; }
.jdbox .item {padding:30rpx}
.wrapbtn1 {width:100%; text-align:center; display:-webkit-box; display:-webkit-flex; display:flex; -webkit-box-align:center; -webkit-align-items:center; align-items:center; justify-content:space-between; padding:0; z-index:10; }
.jdboxmask {background:rgba(0,0,0,.6); z-index:10; position:fixed; width:100%; height:100%; bottom:80rpx; box-sizing:border-box; }
.noteRadio{ overflow: hidden;}
.noteRadio view{float:left; font-size:28rpx; margin-right:20rpx}
.noteRadio view image{width:35rpx; height: 35rpx; float:left; margin-right:10rpx;}

.noteText{height: 250rpx; width:100%; background:#efeff4; margin:0 0 20px 0;}
.statusBtn{width:100%;height: 100rpx; line-height: 100rpx;background:#00a0e9; color:#fff; border-radius:0px; border:0;} 
.statusBtn1 {background:#fff;color:#0f8af4;}

