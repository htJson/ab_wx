<!--pages/news/news.wxml-->
<view class="container">
  <view class="item" wx:for="{{newsList}}">
    <view class="overflow pb20">
      <image class="newsIcon" src="../../images/systemNews.png"></image>
      <text class="newsTitle">{{item.msg_title}}</text><text class="newsTime">{{item.create_datetime}}</text>
    </view>
    <view>{{item.msg_content}}</view>
  </view>
  
</view>
